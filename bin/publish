#!/usr/bin/env node

var process = require('child_process');

process.exec('rm -rf ./public/img/ ./public/css/ ./public/js/ rm -f ./public/index.html ',function (error, stdout, stderr) {
        if (error !== null) {
          console.log('rm -rf public error: ' + error);
        }
        console.log('stdout:',stdout);
}); 

